<?xml version="1.0" encoding="UTF-8"?>
<!-- This is a WordPress eXtended RSS file generated by iHere Blog as an export of your blog. -->
<!-- It contains information about your blog's posts, comments, and categories. -->
<!-- You may use this file to transfer that content from one site to another. -->
<!-- This file is not intended to serve as a complete backup of your blog. -->

<!-- To import this information into a WordPress blog follow these steps. -->
<!-- 1. Log into that blog as an administrator. -->
<!-- 2. Go to Manage: Import in the blog's admin panels. -->
<!-- 3. Choose "WordPress" from the list. -->
<!-- 4. Upload this file using the form provided on that page. -->
<!-- 5. You will first be asked to map the authors in this export file to users -->
<!--    on the blog.  For each author, you may choose to map to an -->
<!--    existing user on the blog or to create a new user -->
<!-- 6. WordPress will then import each of the posts, comments, and categories -->
<!--    contained in this file into your blog -->

<!-- generator="iHere Blog" -->
<rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:wp="http://wordpress.org/export/1.0/"
>
<channel>
	<title>iHere Blog</title>
	<link>{{base_url}}</link>
	<description>iHere-Python,GoogleAppEngine,Django,Java,MyLife</description>
    <pubDate>{% now "r" %}</pubDate>
	<generator>iHere Blog</generator>
	<language>en</language>
	<wp:wxr_version>1.0</wp:wxr_version>
	<wp:base_site_url>{{base_url}}</wp:base_site_url>
	<wp:base_blog_url>{{base_url}}</wp:base_blog_url>    
  {%for cate in categories%}
  <wp:category><wp:category_nicename>{{cate.slug}}</wp:category_nicename><wp:category_parent></wp:category_parent><wp:cat_name><![CDATA[{{cate.name}}]]></wp:cat_name></wp:category>
  {%endfor%}    
  {%for tag in tags%}
  <wp:tag><wp:tag_slug>{{tag.name}}</wp:tag_slug><wp:tag_name><![CDATA[{{tag.name}}]]></wp:tag_name></wp:tag>
  {%endfor%}
{%for item in items %}
<item>
<title>{{item.post.title}}</title>
<link>{{base_url}}{{item.post.get_permalink}}</link>
<pubDate>{{item.post.date}}</pubDate>
<dc:creator><![CDATA[{{item.post.author}}]]></dc:creator>
<category><![CDATA[{{item.post.category}}]]></category>
{%for tag in item.tags%}
<category domain="tag" nicename="{{tag.slug}}"><![CDATA[{{tag.name}}]]></category>
{%endfor%}
<guid isPermaLink="true">{{base_url}}{{item.post.get_permalink}}</guid>
<description></description>
{%autoescape off%}
<content:encoded><![CDATA[{{item.post.content}}]]></content:encoded>
{%endautoescape%}
<wp:post_id>{{item.post.key.id}}</wp:post_id>
<wp:post_date>{{item.post.date|date:"Y-m-d H:i:s"}}</wp:post_date>
<wp:post_date_gmt>{{item.post.date|date:"Y-m-d H:i:s"}}</wp:post_date_gmt>
<wp:comment_status>open</wp:comment_status>
<wp:ping_status>open</wp:ping_status>
<wp:post_name>{{item.post.slug}}</wp:post_name>
<wp:status>{%if item.post.isPublished %}publish{%else%}draft{%endif%}</wp:status>
<wp:post_parent>0</wp:post_parent>
<wp:menu_order>0</wp:menu_order>
<wp:post_type>{{item.post.entrytype}}</wp:post_type>
<wp:post_password></wp:post_password>
{%for comment in item.comments%}
<wp:comment>
<wp:comment_id>{{comment.key.id}}</wp:comment_id>
<wp:comment_author><![CDATA[{{comment.user}}]]></wp:comment_author>
<wp:comment_author_email>{{comment.authorEmail}}</wp:comment_author_email>
<wp:comment_author_url>{{comment.authorWebsite}}</wp:comment_author_url>
<wp:comment_author_IP>{{comment.userIp}}</wp:comment_author_IP>
<wp:comment_date>{{comment.date|date:"Y-m-d H:i:s"}}</wp:comment_date>
<wp:comment_date_gmt>{{comment.date|date:"Y-m-d H:i:s"}}</wp:comment_date_gmt>
{%autoescape off%}
<wp:comment_content><![CDATA[{{comment.content}}]]></wp:comment_content>
{%endautoescape%}
<wp:comment_approved>1</wp:comment_approved>
<wp:comment_type></wp:comment_type>
<wp:comment_parent>0</wp:comment_parent>
<wp:comment_user_id>0</wp:comment_user_id>
</wp:comment>
{%endfor%}
</item>
{%endfor%}
</channel>
</rss>
